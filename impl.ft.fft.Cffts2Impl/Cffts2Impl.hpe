A;                    for(j = 0; j &lt; d2; j++) &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;{&#xA;                        for(i = 0; i &lt; fftblock; i++) &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;{&#xA;                            io = ((k * d2 + j) * d1 + (i + ii)) * 2;&#xA;&#xA;                            int m1 = (io % size1);&#xA;                            int m2 = (m1 % size2);&#xA;                            int _i = io/size1;&#xA;                            int _j = m1/size2;&#xA;                            int _k = m2/2;&#xA;&#xA;                            xout[_i, _j, _k, REAL] = y[0, j, i, REAL];&#xA;                            xout[_i, _j, _k, IMAG] = y[0, j, i, IMAG];&#xA;                        }&#xA;                    }&#xA;                }&#xA;            }&#xA;&#x9;&#x9;    &#xA;&#x9;&#x9;}&#xA;&#x9;&#x9;&#xA;        public int ilog2(int n) {&#xA;            int nn, lg;&#xA;            if(n == 1) {&#xA;                &#xA;            }&#xA;            lg = 1;&#xA;            nn = 2;&#xA;            while(nn &lt; n) {&#xA;                nn = nn * 2;&#xA;                lg = lg + 1;&#xA;            }&#xA;            return lg;&#xA;        }&#xA;&#x9;&#x9;public void setParameters(int dir, int d1, int d2, int d3, double[, , ,] x, double[, , ,] xout){&#xA;&#x9;&#x9;    this.dir  = dir;&#xA;&#x9;&#x9;    this.d1   = d1;&#xA;&#x9;&#x9;    this.d2   = d2;&#xA;&#x9;&#x9;    this.d3   = d3;&#xA;&#x9;&#x9;    this.x    = x;&#xA;&#x9;&#x9;    this.xout = xout;&#xA;&#x9;&#x9;}&#xA;&#x9;}&#xA;}&#xA;" fileType="library" name="ICffts2Impl.cs" srcType="user" uri="impl.ft.fft.Cffts2Impl/bin/1.0.0.0/ICffts2Impl.dll" versionId="1.0.0.0">
